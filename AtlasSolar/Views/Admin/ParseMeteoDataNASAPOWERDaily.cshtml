
@{
    ViewBag.Title = "ParseMeteoDataNASAPOWERDaily";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ParseMeteoDataNASAPOWERDaily</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>ParseMeteoDataNASAPOWERDaily</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        <div class="form-group">
            @Html.Label(AtlasSolar.Resources.Common.Year, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("Year", null, htmlAttributes: new { @class = "form-control" })
            </div>
        </div>

        <div class="form-group">
            @Html.Label(AtlasSolar.Resources.Common.Month, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("Month", null, htmlAttributes: new { @class = "form-control" })
            </div>
        </div>

        <input type="submit" value="ParseMeteoDataNASAPOWERDaily" class="btn btn-default" />

    </div>
}

@if (ViewBag.Report != null)
{
    if (ViewBag.Report != "")
    {
        <br />
        @Html.Raw(ViewBag.Report)
    }
}